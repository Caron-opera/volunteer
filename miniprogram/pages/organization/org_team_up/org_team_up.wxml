<form bindsubmit="submit">
<view class="box">
  <view class="name">
    <view>团队名称：</view>
    <input placeholder="请输入团队名称" name="name"></input>
  </view>
  <view class="line"></view>
  <view class="leader">
    <view>团队负责人：</view>
    <input placeholder="请填入负责人" name="leader"></input>
  </view>
  <view class="line"></view>
  <view class="phone">
    <view>团队负责人手机号码：</view>
    <input placeholder="请输入负责人手机号码" name="phone"></input>
  </view>
  <view class="line"></view>
  <view class="team_loginid">
    <view>团队登录账号：</view>
    <input placeholder="请设置团队登录账号" name="team_loginid"></input>
  </view>
  <view class="line"></view>
  <view class="password">
    <view>团队登录密码：</view>
    <input placeholder="请设置团队登录密码" name="password"></input>
  </view>
  <view class="line"></view>
  <button form-type="submit" type="primary">提交</button>
</view>
</form>
